        ["Key17"] = true,
        ["Key18"] = true,
        ["Key19"] = true,
        ["Key20"] = true,
        ["Key21"] = true,
        ["Key22"] = true,
        ["Key23"] = true,
        ["Key24"] = true,
        ["Key25"] = true,
        ["Key26"] = true,
        ["Key27"] = true,
        ["Key28"] = true,
        ["Key29"] = true,
        ["Key30"] = true,
        ["Key31"] = true,
        ["Key32"] = true,
    }

    if whitelistKeys[key] then
        KeySystem.Visible = false

        ------ Script Here ---------

        game.StarterGui:SetCore("SendNotification", {
            Title = "Creator"; -- the title (ofc)
            Text = "Made By : cool cow / stick cow#5297"; -- what the text says (ofc)
            Icon = "http://www.roblox.com/asset/?id=8531473603"; -- the image if u want. 
            Duration = 5; -- how long the notification should in secounds
        })
        
        game.StarterGui:SetCore("SendNotification", {
            Title = "Version"; -- the title (ofc)
            Text = "Key System V.05"; -- what the text says (ofc)
            Icon = "http://www.roblox.com/asset/?id=8531473603"; -- the image if u want. 
            Duration = 5; -- how long the notification should in secounds
        })
        
        print("Successfully Loaded!") --- Succes Loaded Print
        print("Argon Discord Copied To Clipboard!") --- Prints Copied Argon Hub Discord
        print("----- Made By Made By : cool cow / stick cow#5297 -----") --- Owner / Creator Of Script
        
        
    else
        game.Players.LocalPlayer:Kick("Error 171 : Kicked For Incorrect Key, Please Try Again!")
    end
    
end)



