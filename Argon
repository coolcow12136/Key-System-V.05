SubmitKey.MouseButton1Down:connect(function()
    local key = KeyHere.Text
    local whitelistKeys = {
        ["Key1"] = true,
        ["Key2"] = true,
        ["Key3"] = true,
        ["Key4"] = true,
        ["Key5"] = true,
        ["Key6"] = true,
        ["Key7"] = true,
